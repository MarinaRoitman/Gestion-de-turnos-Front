{
  "expo": {
    "name": "mi-turnito",
    "slug": "mi-turnito",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon-turnito.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-turnito.png",
      "resizeMode": "cover",
      "backgroundColor": "#4F3680"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon-turnito.png",
        "backgroundColor": "#4F3680"
      },
      "edgeToEdgeEnabled": true
    },
    "plugins": [
      "expo-localization"
    ]
  }
}
